				<key>applescript</key>
				<string>on alfred_script(q)	set output to "Failed"	tell application "System Events"		tell current location of network preferences			set VPNservice to service q			set isConnected to connected of current configuration of VPNservice			if isConnected then				disconnect VPNservice				set output to "Disconnected from " &amp; q			else				connect VPNservice				set output to "Connected to " &amp; q			end if		end tell	end tell	return outputend alfred_script</string>
